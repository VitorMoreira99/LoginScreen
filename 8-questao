#include <stdio.h>



int main() {
    float raio, area, comprimento;
float pi=314159;
    printf("Digite o raio da circunferência: ");
    scanf("%f", &raio);

    area = PI * raio * raio;
    comprimento = 2 * PI * raio;

    printf("A área da circunferência é: %.2f\n", area);
    printf("O comprimento da circunferência é: %.2f\n", comprimento);

    return 0;
}
