#include <stdio.h>

int main() {
    float num1, num2;

    printf("Digite o primeiro número: ");
    scanf("%f", &num1);

    printf("Digite o segundo número: ");
    scanf("%f", &num2);

    if (num2 != 0) {
        float divisao = num1 / num2;
        printf("A divisão de %.2f por %.2f é: %.2f\n", num1, num2, divisao);
    } else {
        printf("Erro: Não é possível dividir por zero.\n");
    }

    return 0;
}
